
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <title>Login V2</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!--===============================================================================================-->
    <link rel="icon" type="image/png" href="../Login_Ress/images/icons/favicon.ico" />
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="../Ress/Login_Ress/vendor/bootstrap/css/bootstrap.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="../Ress/Login_Ress/fonts/font-awesome-4.7.0/css/font-awesome.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="../Ress/Login_Ress/fonts/iconic/css/material-design-iconic-font.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="../Ress/Login_Ress/vendor/animate/animate.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="../Ress/Login_Ress/vendor/css-hamburgers/hamburgers.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="../Ress/Login_Ress/vendor/animsition/css/animsition.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="../Ress/Login_Ress/vendor/select2/select2.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="../Ress/Login_Ress/vendor/daterangepicker/daterangepicker.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="../Ress/Login_Ress/css/util.css">
    <link rel="stylesheet" type="text/css" href="../Ress/Login_Ress/css/main.css">
    <!--===============================================================================================-->
    <link href="https://fonts.googleapis.com/css?family=Roboto" rel="stylesheet" type="text/css">
    <script src="https://apis.google.com/js/api:client.js"></script>

</head>
<body>
    <h1 class="headernav"><a href="" class="h1a">Thales</a></h1>
    <nav>

        <div>
            <a class="clicked">Log In</a>
            <a href="" class="notclicked">Product</a>
            <a href="" class="notclicked">About Us</a>
            <a href="" class="notclicked">Contact Us </a>
        </div>
    </nav>
    <div class="limiter">
        <div class="container-login100">
            <div class="wrap-login100">

               @using (Html.BeginForm("SignIn", "Login",
         new { ReturnUrl = ViewBag.ReturnUrl },
         FormMethod.Post, new { @class = "login100-form validate-form" }))
               {
                  
                }
                <form class="login100-form validate-form">
                    <span class="login100-form-title p-b-26">
                        Welcome
                    </span>
                    <span class="login100-form-title p-b-48">
                        <i class="zmdi zmdi-font"></i>
                    </span>
                    <div class="wrap-input100 validate-input" data-validate="Valid email is: a@b.c">
                        <input class="input100" type="text" name="email">
                        <span class="focus-input100" data-placeholder="Email"></span>
                    </div>
                    <div class="wrap-input100 validate-input" data-validate="Enter password">
                        <span class="btn-show-pass">
                            <i class="zmdi zmdi-eye"></i>
                        </span>
                        <input class="input100" type="password" name="pass">
                        <span class="focus-input100" data-placeholder="Password"></span>
                    </div>
                    <div class="container-login100-form-btn">
                        <div class="wrap-login100-form-btn">
                            <div class="login100-form-bgbtn"></div>
                            <button class="login100-form-btn">
                                Login
                            </button>
                        </div>
                    </div>
                    <! google and facebook sign in buttons>
                    <div id="gSignInWrapper">
                        <span class="label"></span>
                        <div id="customBtn" class="customGPlusSignIn">
                            <span class="icon"></span>
                            <span class="buttonText">Google</span>
                        </div>
                    </div>


                    <div class="text-center p-t-115">
                        <span class="txt1">
                            Don’t have an account?
                        </span>
                        <a class="txt2" href="#">
                            Sign Up
                        </a>
                    </div>
                </form>
            </div>
        </div>
    </div>

    <div id="dropDownSelect1"></div>

    <!--===============================================================================================-->
    <script src="../Ress/Login_Ress/vendor/jquery/jquery-3.2.1.min.js"></script>
    <!--===============================================================================================-->
    <script src="../Ress/Login_Ress/vendor/animsition/js/animsition.min.js"></script>
    <!--===============================================================================================-->
    <script src="../Ress/Login_Ress/vendor/bootstrap/js/popper.js"></script>
    <script src="../Ress/Login_Ress/vendor/bootstrap/js/bootstrap.min.js"></script>
    <!--===============================================================================================-->
    <script src="../Ress/Login_Ress/vendor/select2/select2.min.js"></script>
    <!--===============================================================================================-->
    <script src="../Ress/Login_Ress/vendor/daterangepicker/moment.min.js"></script>
    <script src="../Ress/Login_Ress/vendor/daterangepicker/daterangepicker.js"></script>
    <!--===============================================================================================-->
    <script src="../Ress/Login_Ress/vendor/countdowntime/countdowntime.js"></script>
    <!--===============================================================================================-->
    <script src="../Ress/Login_Ress/js/main.js"></script>
</body>
</html>
